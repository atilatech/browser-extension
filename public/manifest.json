{
  "name": "Atila Atlas",
  "description": "Search and save crypto and web3 articles on the internet",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Save this Article"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
      "16": "atila-logo-gradient-128.png",
      "48": "atila-logo-gradient-128.png",
      "128": "atila-logo-gradient-128.png"
  }
}