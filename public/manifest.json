{
  "name": "Atila Atlas",
  "description": "Search and save crypto content on the internet.",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "popup.html",
      "default_title": "Save this Article"
  },
  "options_page": "index.html",
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["./static/js/content.js"]
    }
 ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
      "16": "atila-logo-gradient-128.png",
      "48": "atila-logo-gradient-128.png",
      "128": "atila-logo-gradient-128.png"
  }
}